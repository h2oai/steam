    "jquery": "^3.0.0",
    "lodash": "^4.13.1",
    "react": "^15.2.1",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "h2oUIKit": "0.2.x"
  },
  "devDependencies": {
    "browser-sync": "^2.13.0",
    "browser-sync-webpack-plugin": "^1.0.3",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "jest-cli": "^13.2.3",
    "node-sass": "^3.8.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.2.0",
    "sass-loader": "^3.2.1",
    "sass-resources-loader": "^1.0.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.13.0",
    "tslint-loader": "^2.1.5",
    "typescript": "^1.8.10",
    "typings": "^1.3.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  },
  "scripts": {
    "postinstall": "typings install",
    "start": "webpack --watch",
    "test": "jest"
  },
  "jest": {
    "testDirectoryName": "tests",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "moduleNameMapper": {
      "^.*\\.scss$": "<rootDir>/empty.module.stub.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/coverage"
    ],
    "testFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-redux/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-addons-test-utils/"
    ],
    "scriptPreprocessor": "<rootDir>/preprocessor.jest.js"
  },
  "author": "",
  "license": "ISC"
}
